{
    "configurations": [
        {
            "type": "java",
            "name": "Launch Passgenerator",
            "request": "launch",
            "mainClass": "secure.secure.util.Passgenerator",
            "projectName": "secure"
        },
        {
            "type": "java",
            "name": "Spring Boot-SecureApplication<secure>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "secure.secure.SecureApplication",
            "projectName": "secure",
            "args": "",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=57872 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=secure"
        }
    ]
}